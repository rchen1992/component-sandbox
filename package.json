{
    "name": "react-typescript-storybook-starter",
    "version": "0.0.1",
    "main": "build/lib/index.js",
    "types": "build/lib/index.d.ts",
    "files": [
        "build/lib"
    ],
    "scripts": {
        "storybook": "start-storybook -p 6006 -c .storybook -s ./public",
        "build": "npm run build-lib && build-storybook",
        "build-lib": "tsc && npm run copy-css-to-lib",
        "build-storybook": "build-storybook && npm run copy-public-to-static",
        "copy-public-to-static": "cp -R public/* storybook-static",
        "copy-css-to-lib": "cpx \"./src/**/*.css\" ./build/lib",
        "test": "jest",
        "test:watch": "jest --watch",
        "postinstall": "yarn autoclean --force"
    },
    "repository": "https://github.com/sw-yx/react-typescript-storybook-starter.git",
    "author": "rchen1992 <raymoc2@uci.edu>",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.5",
        "@babel/preset-env": "^7.1.5",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^4.0.4",
        "@storybook/addon-info": "^4.0.4",
        "@storybook/addon-knobs": "^4.0.4",
        "@storybook/addon-storyshots": "^4.0.4",
        "@storybook/react": "^4.1.18",
        "@testing-library/react": "^8.0.4",
        "@types/jest": "^23.3.9",
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.4",
        "@types/react-test-renderer": "^16.0.3",
        "@types/storybook__addon-actions": "^3.4.1",
        "@types/storybook__addon-info": "^3.4.2",
        "@types/storybook__addon-knobs": "^3.4.1",
        "@types/storybook__addon-storyshots": "^3.4.3",
        "@types/storybook__react": "^4.0.0",
        "@types/styled-components": "^4.0.3",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.4",
        "babel-plugin-styled-components": "^1.8.0",
        "cpx": "^1.5.0",
        "jest": "^29.3.1",
        "jest-styled-components": "^6.3.1",
        "react-docgen-typescript-webpack-plugin": "^1.1.0",
        "react-test-renderer": "^16.6.1",
        "ts-jest": "^23.10.4",
        "tslint": "^5.11.0",
        "tslint-microsoft-contrib": "^5.2.1",
        "typescript": "^3.1.6",
        "typescript-plugin-styled-components": "^1.0.0"
    },
    "dependencies": {
        "polished": "^2.3.0",
        "react": "^16.7.0-alpha.2",
        "react-dom": "^16.8.6",
        "styled-components": "^4.0.3"
    }
}
